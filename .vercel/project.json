{
  "framework": "nextjs",
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "frontend/.next",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/backend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}
